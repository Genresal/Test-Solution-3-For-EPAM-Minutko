@model UserViewModel
@{
    ViewData["Title"] = "Home page";
}

<h3>@ViewData["Title"]</h3>

<div class="row">

    <div class="col-md-2">
        <div class="card card-primary card-outline">
            <div class="card-header">
                <h3 class="card-title">User</h3>
            </div>
            <!-- /.card-header -->
            <div class="card-body">
                @await Html.PartialAsync("_UserDetails.cshtml", Model))
                <a href="/Patients/AddOrEdit?id=@Model.Id" class="btn btn-primary btn-block"><b>Edit profile</b></a>
            </div>
        </div>
    </div>
</div>



